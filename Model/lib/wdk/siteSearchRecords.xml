        <columnAttribute name="authors"/>
      </table>

      <table name="GeneLinkouts"
             displayName="External links"
             queryRef="GeneTables.gene_dbrefs">
        <columnAttribute name="ext_id"/>
      </table>

      <table name="PdbSimilarities"
