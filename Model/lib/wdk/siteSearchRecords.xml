          <value>PlasmoDB</value>
        </propertyList>
        <columnAttribute name="rmgmid"/>
        <columnAttribute name="pubmed_id"/>
        <columnAttribute name="mod_type"/>
        <columnAttribute name="life_cycle_stage"/>
