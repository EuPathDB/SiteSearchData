      </table>

      <table name="Epitopes"
             displayName="Epitopes from IEDB"
             queryRef="GeneTables.Epitopes">
        <columnAttribute name="iedb_id"  displayName="Epitope"/>
      </table>

      <table name="Products"
             displayName="All product descriptions"
             queryRef="GeneTables.Products">
